<style>
    .default {
        padding: .5in;
        font-size: 11px;
        font-family: 'Times', serif;
    }
    .default .head {
        text-align: center;
        font-family: sans-serif;
        font-size: 24px;
    }
    .default .sub-head {
        text-align: center;
        margin-bottom: 16px;
        margin-top: 5px;
    }
    .default p {
        margin: 0px;
        font-weight: bold;
        font-size: 11px;
    }
    .default hr {
        margin: 5px 0px;
        background: black
    }
    .default .item {
        margin-bottom: 10px;
        line-height: 1.6em;
    }
    .default .item .title {
        font-weight: bold;
        font-size: 11px;
    }
</style>
<div class="default">
    <div class="head">
        {{firstName}} {{lastName}}
    </div>
    <div class="sub-head">
        {{email}} â€¢ {{phoneNumber}}
    </div>

    <div ng-if="data.work.length > 0">
        <p>Work</p>
        <hr />
        {{#each work}}
        <div class="item">
            <span class="title">{{this.title}}</span>,
            {{this.origin}}
            <div class="data">
                {{this.dateFromString}} - {{this.dateToString}}
            </div>
            {{this.description}}
        </div>
        {{/each}}
    </div>

    <div ng-if="data.ed.length > 0">
        <p>Education</p>
        <hr />
        {{#each ed}}
        <div class="item">
            <span class="title">{{this.title}}</span>,
            {{this.origin}}, {{this.dateString}}
        </div>
        {{/each}}
    </div>

    <div ng-if="data.project.length > 0">
        <p>Projects</p>
        <hr />
        {{#each project}}
        <div class="item">
            <span class="title">{{this.title}}</span>,
            {{this.dateFromString}} - {{this.dateToString}}<br />
            {{this.description}}
        </div>
        {{/each}}
    </div>

    <div ng-if="data.honor.length > 0">
        <p>Honors</p>
        <hr/>
        {{#each honor}}
        <div class="item">
            <span class="title">{{this.title}}</span>,
            {{this.origin}}, Awarded {{this.dateString}}
        </div>
        {{/each}}
    </div>

    <div ng-if="data.skills.length > 0">
        <p>Skills</p>
        <hr />
        {{#each skills}}
        <span ng-repeat="item in data.skills">
            {{this.title}}
        </span>
        {{/each}}
    </div>
</div>
